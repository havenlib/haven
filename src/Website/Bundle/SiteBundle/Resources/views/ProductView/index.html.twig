{%  extends "::base.html.twig" %}
{% block content %}
<dl>
        {% for entity in entities %}
        <table>
            <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ entity.getId() }}</td>
                </tr>
                <tr>
                    <th>Status</th>
                    <td>{{ entity.getStatus() }}</td>
                </tr>
                <tr>
                    <th>GenericProductname</th>
                    <td>{{ entity.getName() }}</td>
                </tr>
                <tr>
                    <th>Description</th>
                    <td>{{ entity.getDescription() }}</td>
                </tr>
                <tr>
                    <th>Price</th>
                    <td>{{ entity.getPrice() }}</td>
                </tr>
            </tbody>
        </table>
        {% else %}
        &nbsp;
        {% endfor %}
    </dl>
    <ul>
    </ul>
{% endblock content %}



