{%  extends "OwnerSiteBundle::base.html.twig" %}
{% import "::macro/route/i18n.html.twig" as route_i18n %}
{% block inside_theater %}
<div id="center-area" class="area">
    <ul>
        {% for entity in entities %}
        <li>
            <img data-src="holder.js/160x160" class="img-polaroid" alt="140x140" style="width: 160px; height: 160px;" src="data:image/png;base64,">
            <a href="{{ route_i18n.path("owner_site_employee_display", {"slug": entity.slug}, ["theteam"])}}">{{ entity.firstname }} - {{ entity.lastname }}</a>
        </li>
        {% endfor %}
    </ul>
</div>
{% endblock inside_theater %}
{% block stylesheets %}
    {{ parent() }}
    {% if app.environment != 'prod' %}
        <link href="{{ asset("less/home_template.less") }}" rel="stylesheet/less" type="text/css" />
    {% else %}
<!-- CSS -->
    {% endif %}
{% endblock stylesheets%}



