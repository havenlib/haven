{%  extends "::base.html.twig" %}
{% block content %}
<ul>
        {% for discriminator_key in discriminator_keys %}
        <li>        
            <a href="{{ path('EvocatioPersonaBundle_PersonaNew', {'discriminator' : discriminator_key}) }}">
                Create a new entry {{ discriminator_key }}
            </a>
        </li>
        {% else %}
        {% endfor %}
    </ul>
</ul>
{% endblock content %}