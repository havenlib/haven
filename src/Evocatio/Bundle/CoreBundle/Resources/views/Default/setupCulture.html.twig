{% extends "::base.html.twig" %}


{% block title %}Info sur la translation{% endblock %}

{% block theatre %}
{{ app.session.flash('notice')|trans }}
<ul>
{% for language in languages %}
<li><a href="{{ path('change_language') }}?lang={{ language.symbol }}" >{{ language.name }}</a></li>
{% endfor %}
</ul>
&nbsp;
{% endblock theatre %}

{% block aside %}

<form action="{{ path('setup_culture_core') }}" method="post" {{ form_enctype(form) }}>
{{ form_widget(form) }}
<input type="submit" value="go" />
</form>

{% endblock aside %}

